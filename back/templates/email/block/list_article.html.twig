<h2>{{ block.title }}</h2>
<table>
    {% for article in block.content %}
        <tr>
            <td>
                {% if article.status and article.status.value is same as constant('App\\ValueObject\\Article\\Status::IN_PROGRESS') %}
                    ðŸ”¥
                {% endif %}
            </td>
            <td>{{ article.title }}</td>
            <td>{{ article.addedAt|date('d/m/Y') }}</td>
            <td>
                {% for sujet in article.sujets %}
                    {{ sujet.label }} -
                {% endfor %}
            </td>
            <td>{{ article.url }}</td>
        </tr>
    {% endfor %}
</table>
